<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="300dp"
    android:layout_height="wrap_content"
    android:paddingTop="30dp"
    android:paddingBottom="30dp"
    tools:context=".ui.home.RentRoomDetailActivity">

    <FrameLayout
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:id="@+id/rent_room_detail_map"
        android:layout_width="match_parent"
        android:layout_height="250dp">


    </FrameLayout>

    <ImageView
        android:id="@+id/rent_room_ok"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:src="@drawable/check"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/rent_room_cancel"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rent_room_detail_map"
        android:layout_marginTop="15dp"
         />

    <ImageView
        android:id="@+id/rent_room_cancel"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:src="@drawable/cancel"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/rent_room_ok"
        app:layout_constraintTop_toBottomOf="@+id/rent_room_detail_map"
        android:layout_marginTop="15dp"
        />

</androidx.constraintlayout.widget.ConstraintLayout>